package ex03;

import java.util.Arrays;

public class ex03 {
	public static void main(String[] args) {
		int[] T1 = new int[10] ;
		String[] T2 = new String[7] ;
		int[] T3 = {0, 6, 2, 4, 3} ;
		String[] T4 = {"bleu","rouge","blanc","vert","mauve","indigo"};
		Arrays.fill(T1, 5);
		Arrays.fill(T2, 1, 2, "bleu");
		System.out.println();
	}

}
